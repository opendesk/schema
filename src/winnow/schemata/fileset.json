{
    "id": "http://opendesk.cc/schemata/fileset.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Winnow Product",
    "type": "object",
    "allOf": [
        {
            "$ref": "http://opendesk.cc/schemata/typed.json#"
        },
        {
            "properties": {
                "version": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    }
                },
                "upstream": {
                    "type": "string"
                },
                "category":{
                    "type": "string"
                },
                "files":{
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "type": {
                    "type": "string",
                    "pattern": "fileset"
                }
            },
            "required": ["version", "files"],
            "additional_properties": false
        }
    ]
}


